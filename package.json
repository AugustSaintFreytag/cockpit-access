{
	"name": "cockpit-access",
	"version": "2.0.1",
	"description": "A thin data access layer, models and interfaces for Cockpit CMS.",
	"repository": "git@gitlab.com:apricum/cockpit-access.git",
	"author": "August Saint Freytag <night@apricummedia.com>",
	"license": "MIT",
	"private": true,
	"source": "src/index.ts",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "$npm_execpath run build-clean && $npm_execpath run build-sources",
		"build-deferred": "scripts/build-deferred.sh",
		"build-clean": "rm -r dist/* || true",
		"build-sources": "ttsc",
		"prepack": "$npm_execpath run build-deferred"
	},
	"dependencies": {
		"isomorphic-fetch": "^3.0.0"
	},
	"devDependencies": {
		"@types/isomorphic-fetch": "^0.0.36",
		"@types/node": "^14.14.22",
		"ts-node": "^10.7.0",
		"ttypescript": "^1.5.12",
		"typescript": "^4.1.3",
		"typescript-transform-paths": "^2.2.3"
	},
	"packageManager": "yarn@3.2.0",
	"files": [
		"dist"
	]
}
